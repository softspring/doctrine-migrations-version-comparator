{
    "name": "softspring/doctrine-migrations-version-comparator",
    "description": "Provides a class to sort doctrine migrations by version name",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "doctrine/migrations": "^3.6",
        "friendsofphp/php-cs-fixer": "3.51.*",
        "phpstan/phpstan": "^1.9"
    },
    "autoload": {
        "psr-4": {
            "Softspring\\Component\\DoctrineMigrationsVersionComparator\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
